# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = space
indent_size = 4
insert_final_newline = false

[*.{js,css,html}]
indent_style = tab

[*.{yml,json,sh}]
indent_size = 2

[{package,typings}.json]
end_of_line = lf

[chinachu]
indent_size = 2
end_of_line = lf